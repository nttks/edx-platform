msgstr "（例）username1@domain.com,gaccotarou,学校 太郎"

#: biz/templates/ga_contract_operation/register_students.html
msgid "ex. username1@domain.com,gaccotarou,gaccoutarou,gacco,Gacco12345"
msgstr "（例）username1@domain.com,gaccotarou,学校 太郎,gacco,Gacco12345"

